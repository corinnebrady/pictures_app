<div class="row">
  <%= render :partial => 'users/sidenav', :locals => {:user => @authenticated} %>
  <div class="col-md-10 col-md-pull-2">
    <div class="row">
      <div class="col-xs-12 col-md-10">
        <h2>My Favourites</h2>
          <% @favourites.each do |favourite| %>
              <%= render :partial => 'pictures/picture', :locals => {:picture => favourite.picture}  %>
          <% end %>
      </div>

        <%= render :partial => 'sidenav', :locals => {:user => @authenticated} %>
    </div>
  </div>

</div>